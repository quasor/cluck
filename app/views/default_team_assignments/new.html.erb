<h1>New default_team_assignment</h1>

<% form_for(@default_team_assignment) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :team_id %><br />
    <%= f.collection_select :team_id, Team.find(:all), :id, :name, {:prompt => :true} %>
  </p>
  <p>
    <%= f.label :state_id %><br />
    <%= f.collection_select :state_id, State.find(:all), :id, :name, {:prompt => :true} %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', default_team_assignments_path %>
