<h1>Listing events</h1>

<table>
  <tr>
    <th>Action</th>
    <th>Release</th>
    <th>Team assignment</th>
    <th>Cluster</th>
    <th>Signed off</th>
    <th>State</th>
    <th>Updated by</th>
  </tr>

<% for event in @events %>
  <tr>
    <td><%=h event.action %></td>
    <td><%=h event.release_id %></td>
    <td><%=h event.team_assignment_id %></td>
    <td><%=h event.cluster_id %></td>
    <td><%=h event.signed_off %></td>
    <td><%=h event.state_id %></td>
    <td><%=h event.updated_by %></td>
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New event', new_event_path %>
