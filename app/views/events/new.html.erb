<h1>New event</h1>

<% form_for(@event) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :action %><br />
    <%= f.text_field :action %>
  </p>
  <p>
    <%= f.label :release_id %><br />
    <%= f.text_field :release_id %>
  </p>
  <p>
    <%= f.label :team_assignment_id %><br />
    <%= f.text_field :team_assignment_id %>
  </p>
  <p>
    <%= f.label :cluster_id %><br />
    <%= f.text_field :cluster_id %>
  </p>
  <p>
    <%= f.label :signed_off %><br />
    <%= f.check_box :signed_off %>
  </p>
  <p>
    <%= f.label :state_id %><br />
    <%= f.text_field :state_id %>
  </p>
  <p>
    <%= f.label :updated_by %><br />
    <%= f.text_field :updated_by %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', events_path %>
